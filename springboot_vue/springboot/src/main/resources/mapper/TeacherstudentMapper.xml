<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ak.springboot.mapper.TeacherstudentMapper">
    <update id="update" parameterType="com.ak.springboot.entity.Teacherstudent">
        update teacherstudent
        <set>
            <if test="status!=null">
                status=#{status},
            </if>
        </set>
        <where>
            id=#{id}
        </where>
    </update>
<!--    <select id="lists" parameterType="com.ak.springboot.entity.Teacherstudent">-->
<!--        select count(state) from teacherstudent where state=#{state}-->
<!--    </select>-->
</mapper>
